  --text-dark: #212529 !important;
  --success-color: #4caf50 !important;
  --danger-color: #f44336 !important;
  --border-radius: 10px !important;
  --card-shadow: 0 4px 12px rgba(0, 0, 0, 0.15) !important;
  --transition: all 0.25s ease !important;
  --spacing-xs: 4px !important;
  --spacing-sm: 8px !important;
  --spacing-md: 12px !important;
  --spacing-lg: 20px !important;
  --font-family: system-ui, -apple-system, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif !important;
  --font-size-sm: 16px !important;
  --font-size-md: 20px !important;
  --font-size-lg: 24px !important;
  --font-size-xl: 32px !important;
}

body {
  font-family: var(--font-family) !important;
  font-size: var(--font-size-md) !important;
  margin: 0 !important;
  padding: 0 !important;
  line-height: 1.4 !important;
  background-color: #f5f8fb !important;
  color: var(--text-dark) !important;
}

body::before {
  content: '' !important;
  position: fixed !important;
  top: 0 !important;
  left: 0 !important;
  right: 0 !important;
  bottom: 0 !important;
  background: url('../imagens/mb2.png') no-repeat center center fixed !important;
  background-size: cover !important;
  opacity: 0.08 !important;
  z-index: -1 !important;
}

/* Header Styling */
#topo {
  background: linear-gradient(135deg, var(--primary-dark), var(--primary-color)) !important;
  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2) !important;
  padding: 10px 20px !important;
  display: flex !important;
  justify-content: space-between !important;
  align-items: center !important;
  color: var(--text-light) !important;
  position: sticky !important;
  top: 0 !important;
  z-index: 100 !important;
  min-height: 70px !important;
}

.navbar-left {
  display: flex !important;
  align-items: center !important;
  gap: 12px !important;
}

.navbar-logo {
  max-height: 70px !important;
  transition: var(--transition) !important;
}

.navbar-text {
  display: flex !important;
  flex-direction: column !important;
}

.navbar-text h1 {
  margin: 0 !important;
  font-size: 26px !important;
  font-weight: 700 !important;
  letter-spacing: 0.5px !important;
}

.navbar-text h6 {
  margin: 0 !important;
  font-size: 16px !important;
  font-weight: 600 !important;
  color: rgba(255, 255, 255, 0.9) !important;
}

.navbar-text p {
  margin: 0 !important;
  font-size: 14px !important;
  font-style: italic !important;
  opacity: 0.9 !important;
}

.navbar-right {
  display: flex !important;
  align-items: center !important;
  gap: 15px !important;
}

/* Radio Section */
.radio-section {
  background: rgba(255, 255, 255, 0.1) !important;
  border-radius: 8px !important;
  padding: 8px 12px !important;
  display: flex !important;
  flex-direction: column !important;
  align-items: center !important;
  backdrop-filter: blur(5px) !important;
